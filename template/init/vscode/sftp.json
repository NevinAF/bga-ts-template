{
	"name": "BGA",
	"host": "1.studio.boardgamearena.com",
	"protocol": "sftp",
	"port": 22,
	"username": "___your-sftp-username___",
	"password": "___your-sftp-password___",
	"remotePath": "/___yourgamename___/",
	"ignore": [
		".vscode",
		".git",
		".gitignore",
		".DS_Store",
		"___source-folder___/.git",
		"___source-folder___/node_modules",
		"___source-folder___/package-lock.json",
		"___source-folder___/nmp-debug.log",
		"___source-folder___/client/build",
		"___source-folder___/template/build.js"
	],
	"syncOption": {
		"skipCreate": false,
		"delete": true
	},
	"uploadOnSave": false,
	"watcher": {
		"files": "**/*",
		"autoUpload": true,
		"autoDelete": true
	}
}