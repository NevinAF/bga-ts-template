{
	"name": "BGA",
	"host": "1.studio.boardgamearena.com",
	"protocol": "sftp",
	"port": 22,
	"remotePath": "/___yourgamename/",
	"ignore": [
		".vscode",
		".idea",
		".git",
		".gitignore",
		".DS_Store",
		"node_modules",
		"package-lock.json",
		"yarn.lock",
		"nmp-debug.log",
		"yarn-error.log",
		"___source-folder___client/build"
	],
	"syncOption": {
		"skipCreate": false,
		"delete": true
	},
	"uploadOnSave": false,
	"watcher": {
		"files": "**/*",
		"autoUpload": true,
		"autoDelete": true
	}
}