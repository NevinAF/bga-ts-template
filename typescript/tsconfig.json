{
	"compilerOptions": {
		"paths": {
			"@cookbook/*": [ "./cookbook/*" ],
		},
		"target": "es5", // NEVER CHANGE THIS
		"module": "amd", // NEVER CHANGE THIS
		"lib": [ "dom", "esnext" ], // Never should need to change.
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedIndexedAccess": true,
		"noEmit": true
	}
}